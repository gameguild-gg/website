{
  "name": "@game-guild/api",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:nodemon": "nodemon src/main.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "migration:generate": "cd src/migrations && npx typeorm-ts-node-commonjs migration:generate -d ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert",
    "schema:drop": "npm run typeorm -- schema:drop"
  },
  "dependencies": {
    "@dataui/crud": "^5.2.4",
    "@dataui/crud-request": "^5.2.4",
    "@dataui/crud-typeorm": "^5.2.4",
    "@dataui/crud-util": "^5.2.4",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sendgrid/client": "^7.7.0",
    "@sendgrid/helpers": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/fs-extra": "^11.0.2",
    "@types/passport-jwt": "^3.0.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "ethers": "^6.4.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "0.3.16",
    "typeorm-transactional": "0.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.194",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
